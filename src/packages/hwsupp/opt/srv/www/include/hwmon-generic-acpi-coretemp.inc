    var3=`cat /sys/devices/system/cpu/cpu2/cpufreq/cpuinfo_cur_freq`
  # var3=`cat /sys/devices/system/cpu/cpu2/cpufreq/scaling_cur_freq`
    var3=$((var3 / 1000))
    var4=`cat /sys/devices/system/cpu/cpu3/cpufreq/cpuinfo_cur_freq`
  # var4=`cat /sys/devices/system/cpu/cpu3/cpufreq/scaling_cur_freq`
    var4=$((var4 / 1000))

    show_tab_header ${_HWSUPP_RT_FREQUENCY} no

    echo "<table class='normtable'>"
    echo "<tr>"
    echo "<th>${_HWSUPP_HWMON_SENSOR}</th>"
    echo "<th>${_HWSUPP_RL_FREQUENCY}</th>"
    echo "</tr>"
    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 0</td>"
    echo "<td>${var1}</td>"
    echo "</tr>"
    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 1</td>"
    echo "<td>${var2}</td>"
    echo "</tr>"
    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 2</td>"
    echo "<td>${var3}</td>"
    echo "</tr>"
    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 3</td>"
    echo "<td>${var4}</td>"
    echo "</tr>"
    echo "</table>"

    show_tab_footer
}


hwmon_generic_acpi_coretemp_coretemp ()
{
    var_coretemp2=`sensors -u coretemp-isa-0000 | grep temp2_input | cut -d : -f 2`
    var_coretemp2=`echo ${var_coretemp2}| sed 's/[^0-9]*\([0-9]*\)\.\([0-9]\{1\}\).*/\1.\2/'`
    var_coretemp3=`sensors -u coretemp-isa-0000 | grep temp3_input | cut -d : -f 2`
    var_coretemp3=`echo ${var_coretemp3}| sed 's/[^0-9]*\([0-9]*\)\.\([0-9]\{1\}\).*/\1.\2/'`
    var_coretemp4=`sensors -u coretemp-isa-0000 | grep temp4_input | cut -d : -f 2`
    var_coretemp4=`echo ${var_coretemp4}| sed 's/[^0-9]*\([0-9]*\)\.\([0-9]\{1\}\).*/\1.\2/'`
    var_coretemp5=`sensors -u coretemp-isa-0000 | grep temp5_input | cut -d : -f 2`
    var_coretemp5=`echo ${var_coretemp5}| sed 's/[^0-9]*\([0-9]*\)\.\([0-9]\{1\}\).*/\1.\2/'`
    show_tab_header Core-${_HWSUPP_TEMP} no
    echo "<table class='normtable'>"

    echo "<tr>"
    echo "<th>${_HWSUPP_HWMON_SENSOR}</th>"
    echo "<th>$Core-${_HWSUPP_RL_TEMP}</th>"
    echo "</tr>"

    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 0</td>"
    echo "<td>${var_coretemp2}</td>"
    echo "</tr>"

    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 1</td>"
    echo "<td>${var_coretemp3}</td>"
    echo "</tr>"

    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 2</td>"
    echo "<td>${var_coretemp4}</td>"
    echo "</tr>"

    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_CPU} 3</td>"
    echo "<td>${var_coretemp5}</td>"
    echo "</tr>"

    echo "</table>"
    show_tab_footer
}

hwmon_generic_acpi_coretemp ()
{
    hwmon_generic_acpi_coretemp_frequency
    hwmon_generic_acpi_coretemp_coretemp
}

