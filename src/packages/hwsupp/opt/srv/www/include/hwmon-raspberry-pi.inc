hwmon_raspberry_pi_temp ()
{
    var1=`sensors -u bcm2835_thermal-virtual-0 | grep temp1_input | cut -d : -f 2`
    var1=`echo ${var1}| sed 's/[^0-9]*\([0-9]*\)\.\([0-9]\{1\}\).*/\1.\2/'`

    show_tab_header ${_HWSUPP_RT_TEMP} no

    echo "<table class='normtable'>"
    echo "<tr>"
    echo "<th>${_HWSUPP_HWMON_SENSOR}</th>"
    echo "<th>${_HWSUPP_RL_TEMP}</th>"
    echo "</tr>"
    echo "<tr align='center'>"
    echo "<td>${_HWSUPP_RC_SOC}</td>"
    echo "<td>${var1}</td>"
    echo "</tr>"
    echo "</table>"

    show_tab_footer
}

hwmon_raspberry_pi ()
{
    hwmon_raspberry_pi_temp
}

