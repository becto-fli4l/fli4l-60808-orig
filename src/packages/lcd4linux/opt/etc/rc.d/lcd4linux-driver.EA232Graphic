#!/bin/sh
: ${LCD4LINUX_DRV_PORT:='ttyS0'}
: ${LCD4LINUX_DRV_SPEED:='115200'}
: ${LCD4LINUX_DRV_MODEL:='KIT240-7'}

echo "    Driver   'EA232graphic'"
echo "    Model    '$LCD4LINUX_DRV_MODEL'"
echo "    Port     '/dev/$LCD4LINUX_DRV_PORT'"
echo "    Speed    '$LCD4LINUX_DRV_SPEED'"
if [ -n "$LCD4LINUX_DRV_CONTRAST" ]
then
  echo "    Contrast '$LCD4LINUX_DRV_CONTRAST'"
fi

