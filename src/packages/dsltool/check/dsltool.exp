DSLTOOLVERSION      = '2\.4\.0' : 'invalid dsltool version'

DSLTOOLMODEMDEMO    = 'demo-adsl|demo-vdsl' : ''
DSLTOOLMODEMTELNET  = 'amazon|ar7|bc63|conexant|openwrt|speedtouch|trendchip|vigor|vinax' : ''
DSLTOOLMODEMHTTP    = 'avm-tr064' : ''
DSLTOOLMODEMTYPE    = '(RE:DSLTOOLMODEMDEMO)|(RE:DSLTOOLMODEMTELNET)|(RE:DSLTOOLMODEMHTTP)'
                    : 'wrong modem type, choose one of amazon, ar7, avm-tr064, bc63, conexant, openwrt, speedtouch, trendchip, vigor, vinax, demo-adsl or demo-vdsl'

DSLTOOLPROTOCOL     = 'telnet|http|demo'
                    : 'wrong protocol type, choose one of telnet, http or demo'
DSLTOOLETHTYPE      = 'auto|IPv4|IPv6'
                    : 'wrong eth type, choose one of IPv4, IPv6 or auto'
                  